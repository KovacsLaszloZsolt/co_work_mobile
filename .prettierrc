{
  "singleQuote": true,
  "trailingComma": "all",
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "arrowParens": "always",
  "bracketSpacing": true,
  "jsxSingleQuote": false,
  "semi": true,
  "quoteProps": "as-needed",
  "proseWrap": "always",
  "plugins": ["prettier-plugin-organize-imports"]
}
